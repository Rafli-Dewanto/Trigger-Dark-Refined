{
  "name": "trigger-dark-refined",
  "displayName": "Trigger Dark Refined",
  "description": "A sleek, modern dark theme for Visual Studio Code inspired by Trigger aesthetics",
  "version": "2.2.1",
  "publisher": "dewantorafli",
  "author": {
    "name": "Rafli Dewanto",
    "email": "swe@dewanto.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/Rafli-Dewanto/Trigger-Dark-Refined#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rafli-Dewanto/Trigger-Dark-Refined"
  },
  "bugs": {
    "url": "https://github.com/Rafli-Dewanto/Trigger-Dark-Refined/issues"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark",
    "trigger",
    "refined",
    "color-theme",
    "dark-theme",
    "modern"
  ],
  "icon": "favicon.png",
  "galleryBanner": {
    "color": "#1a1a1a",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Trigger Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/trigger-dark.json"
      },
      {
        "label": "Trigger Dark Midnight",
        "uiTheme": "vs-dark",
        "path": "./themes/trigger-dark-midnight.json"
      },
      {
        "label": "Trigger.darker",
        "uiTheme": "vs-dark",
        "path": "./themes/trigger-darker.json"
      },
      {
        "label": "Trigger",
        "uiTheme": "vs-dark",
        "path": "./themes/trigger.json"
      }
    ]
  },
  "scripts": {
    "build": "vsce package",
    "publish": "vsce publish",
    "package": "vsce package",
    "vscode:prepublish": "echo 'Preparing for publish...'",
    "prepackage": "echo 'Pre-packaging checks...'",
    "postpackage": "echo 'Package created successfully!'",
    "login": "vsce login",
    "show": "vsce show dewantorafli.trigger-dark-refined",
    "setup": "./setup.sh"
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  }
}
