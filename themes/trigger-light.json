  "tokenColors": [
    {
      "name": "HTML & CSS",
      "scope": [
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html",
        "string.quoted.double.html",
        "string.quoted.double.css"
      ],
      "settings": {
        "foreground": "#9ED66B"
      }
    },
    {
      "name": "Asynchronous Programming Keywords JS",
      "scope": ["storage.modifier.async.js", "storage.modifier.async.ts"],
      "settings": {
        "foreground": "#D886FF"
